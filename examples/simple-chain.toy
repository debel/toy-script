  (http "")
  (json "")
  (stdio "")
)

(@var
  (fetch (@chain
    http.get
    json.parse
    (@func (json_hash) (
      (@match
        (@has json_hash "posts")
        (@when true (@get json_hash "posts"))
        (@when false json_hash)
      )
